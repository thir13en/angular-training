{
  "name": "angular-training",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "install:fresh": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm shrinkwrap && npm run start",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint --format=stylish",
    "lint:fix": "ng lint --fix",
    "check": "ng build && ng build --prod && ng lint && npm run test:single",
    "e2e": "ng e2e",
    "dev:ssr": "ng run angular-training:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build --prod && ng run angular-training:server:production",
    "prerender": "ng run angular-training:prerender"
  },
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/platform-server": "~10.1.0",
    "@angular/router": "~10.1.0",
    "normalize.css": "~8.0.1",
    "rxjs": "~6.6.2",
    "tslib": "~2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@nguniversal/builders": "~10.1.0",
    "@nguniversal/express-engine": "~10.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.7",
    "codelyzer": "~6.0.0",
    "husky": "~4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check"
    }
  }
}
